{
  "ignorePatterns": ["dist"],
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/essential",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "parser": "vue-eslint-parser",
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {}
}
